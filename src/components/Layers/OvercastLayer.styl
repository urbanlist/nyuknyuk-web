.overcast-layer
  width 100%
  height 100%
  
  @keyframes overcast
    from
      transform translate(-164px, 0px)
    to
      transform translate(0px, 0px)

  .overcast-item
    background-image url('../../../assets/weather/cloud-cloudy-body.svg')
    background-size auto
    width 150%
    height 180px
    border-style none